{
  "ConnectionStrings": {
    "PostgresTemplate": "Host=localhost;Port=5432;Database=pokemon_portal_db;User Id={userName};Password={password};",
    /*We don't use cache in local envs*/
    "RedisTemplate": "***",
    "SqlServerTemplate": "Server=tcp:pokemon-portal-prd-db.database.windows.net,1433;Initial Catalog=pokemon-portal-prd-db;Persist Security Info=False;User ID={userName};Password={password};MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"

  },

  "Cors": {
    "AllowedOrigins": [
      "http://localhost:3000",
      "https://localhost:7204",
      "http://localhost:5214",
      "http://localhost:5651"
    ]
  }
}